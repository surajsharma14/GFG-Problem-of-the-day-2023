 int arrayOperations(int n, vector<int> &arr) {
       
        int ans=0;
        bool check=false;
        int i=0;
        while(i<n){
           if(arr[i]!=0){
               ans++;
               while(arr[i]!=0){
                   i++;
               }
           }
           else{
               check=true;
               i++;}
        }
        if(check==true)
            return ans;
       else
            return -1;
    }
